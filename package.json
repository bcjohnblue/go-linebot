{
  "name": "go-linebot",
  "version": "1.0.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "description": "LINE Bot for Go game analysis",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
