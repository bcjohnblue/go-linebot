  "license": "MIT",
  "dependencies": {
    "@google-cloud/compute": "^6.6.0",
    "@google-cloud/storage": "^7.18.0",
    "@line/bot-sdk": "^10.5.0",
    "@sabaki/sgf": "^3.4.7",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "openai": "^6.16.0",
    "uuid": "^13.0.0"
  }
}
