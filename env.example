# LINE Bot Configuration
LINE_CHANNEL_ACCESS_TOKEN=your_line_channel_access_token
LINE_CHANNEL_SECRET=your_line_channel_secret

# GCP Configuration
GCP_PROJECT_ID=your_gcp_project_id
GCP_ZONE=asia-east1-a
GCP_SERVICE_ACCOUNT_KEY_PATH=./gcp-service-account-key.json

# VM Configuration
VM_INSTANCE_NAME_PREFIX=katago-worker
VM_MACHINE_TYPE=n1-standard-2
VM_IMAGE_PROJECT=ubuntu-os-cloud
VM_IMAGE_FAMILY=ubuntu-2204-lts
VM_DISK_SIZE_GB=20

# Storage Configuration (for SGF files and results)
GCS_BUCKET_NAME=go-linebot-storage

# KataGo Configuration
KATAGO_SCRIPT_PATH=/home/ubuntu/analyze.sh
KATAGO_RESULT_PATH=/home/ubuntu/result.txt

# Server Configuration
PORT=3000
WEBHOOK_PATH=/webhook
PUBLIC_URL=https://your-public-domain.com

# Minimax Configuration
MINIMAX_API_KEY=your_minimax_api_key
MINIMAX_BASE_URL=https://api.minimax.chat/v1

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_BASE_URL=https://api.openai.com/v1

